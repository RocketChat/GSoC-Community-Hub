{
  "name": "@palanikannan1437/rc4community-hackernews",
  "version": "0.0.1",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf dist .turbo node_modules",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "lucide-react": "^0.115.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "tailwind-merge": "^1.9.1",
    "tailwindcss-animate": "^1.0.5"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "tailwindcss": "^3.2.4",
    "tailwindcss-animate": "^1.0.5",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "postcss": "^8.4.24"
  },
  "typesVersions": {
    "*": {
      "functions": [
        "dist/functions.d.mts"
      ],
      "HackerNewsComponent": [
        "dist/HackerNewsComponent.d.mts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./HackerNewsComponent": {
      "import": "./dist/HackerNewsComponent.mjs",
      "types": "./dist/HackerNewsComponent.d.mts"
    },
    "./functions": {
      "import": "./dist/functions.mjs",
      "types": "./dist/functions.d.mts"
    }
  }
}